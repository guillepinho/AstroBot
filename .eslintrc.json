{
  "root": true,
  "env": {
    "browser": false,
    "commonjs": true,
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "camelcase": "warn",
    "max-len": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off"
  }
}